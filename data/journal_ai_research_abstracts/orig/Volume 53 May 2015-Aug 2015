<meta name="citation_author" content="Krysta, Piotr">
<meta name="citation_author" content="Telelis, Orestis">
<meta name="citation_author" content="Ventre, Carmine">
<meta name="citation_publication_date" content="2015">
<meta name="citation_journal_title" content="Journal of Artificial Intelligence Research">
<meta name="citation_firstpage" content="721">
<meta name="citation_lastpage" content="744">
<meta name="citation_volume" content="53">
